entity Project {
	name String,
    description String,
    startDate Instant,
    endDate Instant,
    isActive Boolean
}

entity Week {
	label String,
    monthNum Integer,
    year Integer,
    weekNum Integer,
    isActive Boolean
}

entity Activity {
	timeSpent Long
}

relationship ManyToOne {
	Activity{project(name)} to Project,
    Activity{User(firstName)} to User,
    Activity{week(label)} to Week,
    Project{User(firstName)} to User
}

filter Activity
filter Project